{{#each flights as |flight|}}

<!-- <table data-id=""> -->
  <tr data-id="{{flight.id}}">
    <td>{{flight.type}}</td>
    <td>{{flight.date}}</td>
    <td>{{flight.time}}</td>
    <td>{{flight.distance}}</td>
    <td>{{flight.duration}}</td>
    <td>{{flight.launch}}</td>
    <td>{{flight.lz}}</td>
    <td>{{flight.wing}}</td>
    <td>{{flight.notes}}</td>
    <td><button class='edit'>edit</button></td>

  </tr>
<!-- </table> -->

{{/each}}
